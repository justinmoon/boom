on late-init
    stop zygote
    stop zygote_secondary
    stop system_server
    start surfaceflinger
    start inputflinger
    start netd
    mkdir /data/local/webos 0755 system system
    chown system system /data/local/webos
    setprop persist.webosd.enabled 1

service webosd /system/bin/webosd
    class main
    user root
    group root system graphics input inet wakelock
    disabled

on property:persist.webosd.enabled=1
    start webosd
