on late-init
    start surfaceflinger
    start inputflinger

service webosd /system/bin/webosd
    class main
    user root
    group root graphics input inet wakelock
    seclabel u:r:su:s0
    disabled

on property:sys.boot_completed=1
    # optional: keep it lean
    stop zygote
    stop system_server
    start webosd
